{
  "name": "specrec-ts",
  "version": "1.0.0",
  "description": "Turn untestable legacy code into comprehensive test suites in minutes - TypeScript implementation of SpecRec",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "testing",
    "legacy-code",
    "dependency-injection",
    "test-doubles",
    "mocking",
    "typescript",
    "tdd",
    "refactoring"
  ],
  "author": "Ivett Ordog",
  "license": "PolyForm-Noncommercial-1.0.0",
  "homepage": "https://github.com/devill/specrec-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devill/specrec-ts.git"
  },
  "bugs": {
    "url": "https://github.com/devill/specrec-ts/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.3",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
